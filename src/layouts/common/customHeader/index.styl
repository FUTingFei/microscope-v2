.customHeader
  width 100%
  height 80px
  background #FFFFFF
  box-shadow 0 5px 4px 0 rgba(170,170,170,0.07)
  position sticky
  top 0
  z-index 888
  .menu
    font-family PingFangSC-Regular
    font-size 16px
    color #47484A
    letter-spacing 0
    text-align center
    margin-left 40px
    display inline-block
  .rc-menu
    z-index 900
    padding 0px
    width 100%
    border none
    box-shadow none
  .rc-menu-submenu
    box-shadow none
  .rc-menu-submenu-title
    padding 0px !important
    color white
    height 34px
    text-align center
    color #332b2b
    width 100px
    background-color white
  .rc-menu-submenu-arrow
    padding-top 5px
