.home
  background-color white
  .generalItem
    color white
    height 150px
    padding-left 15px
    padding-right 15px
    margin-top 30px
    >div:nth-child(1)
      border-radius 5px 5px
      background-size cover
      width 100%
      height 100%
    // &:nth-child(1)
    //   padding-left 0
    //   padding-right 20px
    // &:nth-child(2)
    //   padding-left 10px
    //   padding-right 20px
    // &:nth-child(3)
    //   padding-left 10px
    //   padding-right 0
    .generalItemIcon
      position absolute
      top 40px
      left 40px
    .generalItem1Label
      position absolute
      top 40px
      left 130px
      font-size 28px
    .generalItem2Label
      position absolute
      top 84px
      left 132px
      font-size 16px

  .blockItem
    height 179px
    width 100%
    margin-top 20px
