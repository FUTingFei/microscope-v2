    border-radius 4px 4px

    &:nth-child(1)
      margin-top 10px
    >div:nth-child(1)
      padding-left 39px
      padding-top 37px
      width 100%
      height 100%
      >div
        display inline-block
        vertical-align top
      >div:nth-child(1)
        width 96px
        height 105px
        background-color blue
        border-radius 5px 5px
      >div:nth-child(2)
        margin-left 15px
        font-size 14px
        width 346px

  .transactionItem
    height 179px
    width 100%
    margin-top 20px
    background-color white
    border-radius 4px 4px
    &:nth-child(1)
      margin-top 10px
